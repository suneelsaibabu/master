package assign;

public class pract {
	int emp;
	int sal;
	private final static String ceo="ramesh";
	
	public void show()
	{
	System.out.println(emp +" " +sal +" " +ceo);
	}
public static void main(String[] args)
{
	pract suneel=new pract();
	pract sumanth=new pract();
	
	suneel.show();
	sumanth.ceo="rem";
	sumanth.show();
}
}

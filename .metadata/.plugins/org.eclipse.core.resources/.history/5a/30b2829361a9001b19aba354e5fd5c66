package Sri;

public class Test2 {
	
	int[] a= {1,2,3,4};
	int[] b= {1,2,3,4};
	
	int result;
	public void result() {
		for(int i=0;i>=a.length-1;i++) {
			for(int j=i+1;i>=a.length-1;j++) {
				 result =a[i]+a[j];
			}
		}
		System.out.println(result);
	}
	public static void main(String[] args) {
		Test2 t=new Test2();
		t.result();
	}

}
